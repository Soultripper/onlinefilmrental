<!DOCTYPE html>
<html>
  <head>
    <title>Onlinefilmrental</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="container">
      <header>
        <%= render :template => 'app/header' %>
      </header>
      <div id="main" role="main">
        <%= yield %>
      </div>
    </div>
    <footer>
          <%= render :template => 'app/footer' %>
    </footer>
  </body>
</html>
