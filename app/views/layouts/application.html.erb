<!DOCTYPE html>
<html>
  <head>
    <title>Onlinefilmrental</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="main">
      <div id="wrapper">
        <header>
          <%= render :template => 'app/header' %>
        </header>
        <%= yield %>
      </div>
      <footer>
        <%= render :template => 'app/footer' %>
      </footer>
    </div>
  </body>
</html>
