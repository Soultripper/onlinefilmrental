<div class="films">
  <% @films.each do |film| %>
    <div class="film">
      <%# film.position %>
      <%=link_to image_tag(film.film.image_uri_medium, :title => film.film.title), :action=>'show', :controller=>'film', :id => film.id  %>
    </div>   
  <% end %>
</div>
<div class="clearing"></div>